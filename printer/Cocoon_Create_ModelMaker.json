{
  "name": "Cocoon Create ModelMaker",
  "machine_name": "Cocoon Create ModelMaker",
  "source": "CuraV2",
  "license": "LGPL-3.0",
  "origin_center": false,
  "bed_elliptic": false,
  "bed_belt": false,
  "extruders": [
    {
      "nozzle": 0.4,
      "filament": 1.75,
      "offset_x": 0.0,
      "offset_y": 0.0,
      "select": "",
      "deselect": ""
    }
  ],
  "build_size": [
    120,
    135,
    100
  ],
  "pre_gcode": [
    "M104 {temp}",
    "M109",
    "; -- START GCODE --",
    "G21                     ;set units to millimetres",
    "G90                     ;set to absolute positioning",
    "M106 S0                 ;set fan speed to zero (turned off)",
    "G28                     ;home all axis",
    "G92 E0                  ;zero the extruded length",
    "G1 Z1 F1000             ;move up slightly",
    "G1 X60.0 Z0 E9.0 F1000.0;intro line",
    "G1 X100.0 E21.5 F1000.0 ;continue line",
    "G92 E0                  ;zero the extruded length again",
    "; -- end of START GCODE --"
  ],
  "post_gcode": [
    "; -- END GCODE --",
    "M104 S0                 ;turn off nozzle heater",
    "G91                     ;set to relative positioning",
    "G1 E-10 F300            ;retract the filament slightly",
    "G90                     ;set to absolute positioning",
    "G28 X0 Y0                 ;move to the XY-axis origin (Home)",
    "M84                     ;turn off stepper motors",
    "; -- end of END GCODE --"
  ],
  "fan_gcode": "",
  "progress_gcode": "",
  "layer_gcode": "",
  "heated_bed": false,
  "speed_travel": 120.0
}