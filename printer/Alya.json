{
  "name": "Alya",
  "machine_name": "ALYA 3DP",
  "source": "CuraV2",
  "license": "LGPL-3.0",
  "origin_center": false,
  "bed_elliptic": false,
  "bed_belt": false,
  "extruders": [
    {
      "nozzle": 0.4,
      "filament": 1.75,
      "offset_x": 0.0,
      "offset_y": 0.0,
      "select": "",
      "deselect": ""
    }
  ],
  "build_size": [
    100,
    100,
    133
  ],
  "pre_gcode": [
    "M104 {temp}",
    "M109",
    ";Sliced at: {day} {date} {time} ",
    " ;Basic settings: Layer height: {layer_height} Walls: {wall_thickness} Fill: {fill_density} ",
    " ;Print time: {print_time} ",
    " ;Filament used: {filament_amount}m {filament_weight}g ",
    " ;Filament cost: {filament_cost} ",
    " G28 X0 Y0  ;move X Y to endstops ",
    " G28 Z0     ;move Z to endstops ",
    " ; M190 S{material_bed_temperature} ;bed temp ",
    " M107 ; switch fan off ",
    " M109 S{temp} ;extruder temp set ",
    " G1 F3000 ",
    " G1 Z10 ",
    " G92 E0                  ;zero the extruded length ",
    " G1 F200 E1 ;extrude 1mm of feed stock ",
    " G92 E0 ;zero the extruded length again ",
    "  G4 P7000 ; wait 7000ms ",
    " M117 Printing... ;Put printing message on LCD screen"
  ],
  "post_gcode": [
    ";End GCode",
    "M104 S0 ;extruder heater off ",
    "M140 S0 ;heated bed heater off (if you have it)",
    "G91 ;relative positioning",
    "G1 E-1 F300 ;retract the filament a bit before lifting the nozzle, to release some of the pressure",
    "G1 Z+0.5 E-5 X-20 Y-20 F120 ;move Z up a bit and retract filament even more",
    "G28 X0 Y0 ;move X/Y to min endstops, so the head is out of the way",
    "G28 Z0",
    "M84 ;steppers off",
    "G90 ;absolute positioning",
    ";{profile_string}"
  ],
  "fan_gcode": "",
  "progress_gcode": "",
  "layer_gcode": "",
  "heated_bed": false,
  "speed_travel": 120.0
}