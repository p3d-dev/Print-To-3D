{
  "name": "JGAurora A6",
  "machine_name": "JGAurora A6",
  "source": "CuraV2",
  "origin_center": false,
  "bed_elliptic": false,
  "bed_belt": false,
  "extruders": [
    {
      "nozzle": 0.4,
      "filament": 1.75,
      "offset_x": 0.0,
      "offset_y": 0.0,
      "select": "",
      "deselect": ""
    }
  ],
  "build_size": [
    300,
    200,
    200
  ],
  "pre_gcode": [
    "M190 S{bed_temp} ;bed temperature line",
    "M109 S{temp} ;temperature line",
    "G21 ;metric values",
    "G90 ;absolute positioning",
    "M82 ;set extruder to absolute mode",
    "M107;start with the fan off",
    "G28 X0 Y0 ;move X/Y to min endstops",
    "G28 Z0 ;move Z to min endstops",
    "; PLEASE CHECK: G1 Z15.0 F{travel_speed} ;move the platform down 15mm",
    "G92 E0 ;zero the extruded length",
    "F200 E3;extrude 3mm of feed stock",
    "G92 E0 ;zero the extruded length again",
    "; PLEASE CHECK: G1 F{travel_speed}",
    "; PLEASE CHECK: G1 Z0.0 F{travel_speed}",
    "M117 Printing... ;LCD Message"
  ],
  "post_gcode": [
    "M104 S0 ;extruder heater off",
    "M140 S0;heated bed heater off",
    "G91;relative positioning",
    "G1 E-1 F300 ;retract the filament a bit before lifting the nozzle, to release some of the pressure",
    "; PLEASE CHECK: G1 Z+0.5 E-5 X-20 Y-20 F{travel_speed} ;move Z up a bit and retract filament even more",
    "G28 X0 Y0;move X/Y to min endstops, so the head is out of the way",
    "M84 ;steppers off",
    "G90;absolute positioning",
    "M104 S0 ; turn off extruder",
    "M140 S0 ; turn off heatbed",
    "M107 ; turn off fan",
    "G1 X178 Y180 F4200 ; park print head",
    "M84 ; disable motors"
  ],
  "fan_gcode": "",
  "progress_gcode": "",
  "layer_gcode": "",
  "heated_bed": true,
  "need_hotend_temp": true,
  "need_bed_temp": true
}